[alias]
  addp = add -p
  br = branch
  ch = cherry-pick
  ci = commit
  cia = commit --amend
  cianoe = commit --amend --no-edit
  co = checkout
  diffs = diff --staged
  dt = difftool
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  logl = log --oneline
  logp = log -p
  logs = log --stat
  mt = mergetool
  pu = pull
  pushf= push --force-with-lease
  re = rebase
  re1 = rebase -i HEAD~1
  re10 = rebase -i HEAD~10
  re2 = rebase -i HEAD~2
  re3 = rebase -i HEAD~3
  re4 = rebase -i HEAD~4
  re5 = rebase -i HEAD~5
  re6 = rebase -i HEAD~6
  re7 = rebase -i HEAD~7
  re8 = rebase -i HEAD~8
  re9 = rebase -i HEAD~9
  recont = rebase --continue
  reom = rebase origin/master
  rs = reset
  rs1 = reset HEAD~
  rs2 = reset HEAD~2
  rs3 = reset HEAD~3
  rs4 = reset HEAD~4
  rs5 = reset HEAD~5
  rsom = reset --hard origin/master
  sh = stash
  shls = stash list
  st = status
[commit]
  gpgsign = true
  verbose = true
[core]
  editor = vim
  excludesfile = ~/.gitignore
[diff]
  context = 5
[merge]
  conflictstyle = diff3
  ff = false
  tool = vimdiff
[mergetool]
  keepBackup = false
  prompt = false
[mergetool "vimdiff"]
  cmd = vimdiff -f "$LOCAL" "$MERGED" "$REMOTE"
[pull]
  rebase = true
  all = true
[push]
  followTags = true
[rebase]
  autoStash = true
[stash]
  showPatch = true
[status]
  showStash = false
